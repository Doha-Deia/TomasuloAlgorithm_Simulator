{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build",
      "type": "shell",
      "command": "C:/not windows/apps set up/mingw64/bin/g++.exe",
      "args": [
        "-std=c++20",
        "-g",
        "main.cpp",
        "ROB.cpp",
        "ReservationStation.cpp",
        "Instructions.cpp",
        "RegisterStatus.cpp",
        "FunctionalUnit.cpp",
        "-o",
        "simulator"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": ["$gcc"]
    },
    {
      "label": "run",
      "type": "shell",
      "command": "${workspaceFolder}/simulator",
      "dependsOn": "build",
      "group": "test"
    }
  ]
}
